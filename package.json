{
  "name": "pos-admin-frontend",
  "version": "1.0.0",
  "description": "POS Admin Panel Frontend with Vue.js and Tailwind CSS",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./auto-deploy.ps1",
    "cleanup": "powershell -ExecutionPolicy Bypass -File ./cleanup.ps1",
    "build:deploy": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.0.18",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.32",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "vite": "^5.0.10",
    "vue-tsc": "^3.0.1"
  }
}
